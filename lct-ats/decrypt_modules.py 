from cryptography.fernet import Fernet

# Замените ключ на ваш действительный ключ в формате base64
key = b'ваш_ключ_здесь_32_байта_в_формате_base64=='

cipher = Fernet(key)

with open('encrypted_file.crypt', 'rb') as file:
    encrypted_data = file.read()

decrypted_data = cipher.decrypt(encrypted_data)

with open('decrypted_module.py', 'wb') as file:
    file.write(decrypted_data)
